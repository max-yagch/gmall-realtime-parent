<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="yang.gmallrealtimepubliseher.mapper.DauMapper">
    <select id="getDauTotal" resultType="java.lang.Integer">
       select count(*)  ct from gmallrealtime0218_dau where logdate=#{date}
   </select>

    <select id="getDauHour" resultMap="dauHourList">
       select loghour, count(*) ct from  gmallrealtime0218_dau where logdate=#{date} group by loghour
   </select>

    <resultMap id="dauHourList" type="java.util.Map" autoMapping="true">
        <!--        <result property="HOUR" column="LOGHOUR"></result>
                <result property="CT" column="CT"></result>-->
    </resultMap>

</mapper>